@using PaiXie.Data
@{
    var objList = ViewBag.objList as List<Sysrole>;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    @section head{

    }
<div data-options="region:'north'" style="height:360px">
    <input type="hidden" id="ck" name="ck" value="@ViewBag.ck" />

    <input type="hidden" value="@ViewBag.UID" id="UID" name="UID" />
    <input type="hidden" value="@ViewBag.UCode" id="UCode" name="UCode" />
    <table id="grid"   class="easyui-datagrid"
