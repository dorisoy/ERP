@{
    ViewBag.Title = "商品品牌";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section head{

}
<div class="easyui-layout" data-options="fit:true">
    <div data-options="region:'north',height:63">
        <div class="toolbar" id="toolbar">
            <a href="javascript:void(0);" class="refresh" id="refresh"><span></span>刷新</a>
            <a href="javascript:void(0);" class="add" id="add"><span></span>添加品牌</a>
            <a href="javascript:void(0);" class="del unclick" id="del"><span></span>删除</a>
            <a href="javascript:void(0);" id="refreshCurrentPage" style="display:none;"><span></span>刷新当前页</a>
        </div>
    </div>
    <div data-options="region:'center'" style="padding:0 22px">
        <table id="grid" class="easyui-datagrid"
               data-options="singleSelect:false,collapsible:true,method:'get'"></table>
    </div>
</div>
@section bottom{
    <script src="~/Areas/Products/ViewJs/BrandInfoList.js"></script>
}
