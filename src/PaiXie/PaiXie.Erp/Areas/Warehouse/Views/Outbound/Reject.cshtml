@{
    var ids = ViewBag.IDs;
    ViewBag.Title = "出库单驳回";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section head{
    <style>
        .tableinfo td {
            height: 20px;
            line-height: 20px;
        }
    </style>
}
<form id="ff" method="post" novalidate="novalidate">
    <input type="hidden" value="@ids" id="hdnIDs" name="ids" />
    <table id="tblAdd" class="tableinfo">
        <tr>
            <td>
                <span style="color:#ff0000;">*</span>输入驳回备注:
            </td>
        </tr>
        <tr>
            <td>
                <textarea id="txtRejectRemark" name="rejectRemark" class="textarea easyui-validatebox" style="width:400px; height:110px;" maxlength="200"></textarea>
            </td>
        </tr>
        <tr>
            <td style="text-align:center;">
                <a href="javascript:void(0);" class="button-oky" id="btnSave" iconcls="icon-ok">驳回</a>
                <a href="javascript:void(0);" class="button-cancel" iconcls="icon-cancel" id="btnClose">取消</a>
            </td>
        </tr>
    </table>
</form>
@section bottom{
    <script src="~/Areas/Warehouse/ViewJs/Reject.js"></script>
}
